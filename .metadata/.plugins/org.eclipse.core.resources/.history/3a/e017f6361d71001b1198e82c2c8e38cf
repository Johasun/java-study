public class S0217_HashMap {
	public static void main(String[] args) {
		String str = "^01(?:0|1|[6-9]) - (?:\\d{3}|\\d{4}) - \\d{4}$";
		
		
		System.out.println("ют╥б");
		Scanner sc = new Scanner(System.in);
