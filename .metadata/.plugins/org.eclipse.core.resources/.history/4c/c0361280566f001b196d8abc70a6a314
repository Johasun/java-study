import java.util.Stack;

/*
JAVA API
Stack	Queue 제공
*/
public class Ex05_Stack_Collection {
	public static void main(String[] args) {
		Stack s = new Stack();
		s.push("A");
		s.push("B");
		System.out.println(s.pop());
		System.out.println(s.pop());
		//나중에 들어온게 먼저 빠짐 >> B A
		//System.out.println(s.pop()); 	>>java.util.EmptyStackException
		System.out.println(s.isEmpty());
	}

}
